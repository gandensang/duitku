<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
	<PackageLicenseExpression>MIT OR Apache-2.0</PackageLicenseExpression>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Budi Santoso</Authors>
    <Company>PT Exrush Digital Nusantara</Company>
    <Description>Duitku payment Gateway</Description>
    <Copyright>MIT</Copyright>
    <RepositoryUrl>https://github.com/gandensang/duitku</RepositoryUrl>
    <PackageTags>duitku, duitku payment gateway</PackageTags>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageProjectUrl>https://github.com/gandensang/duitku</PackageProjectUrl>
    <Version>1.0.7</Version>
    <PackageReleaseNotes>- update payment method</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
  </ItemGroup>

</Project>
